{
  "expo": {
    "name": "Countdown",
    "slug": "Countdown",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/countdown_logo.png",
    "scheme": "Countdown",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/countdown.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.schandu.Countdown"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/notification_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification_icon.png",
          "color": "#ffffff",
          "defaultChannelId": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/countdown.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FEF7FF",
          "dark": {
            "image": "./assets/images/countdown.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#1C1B1F"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "17af7f41-05a1-426b-b4fd-e5493d318402"
      }
    }
  }
}
